/// Acumen application configuration
/// Used by generate-k8s.rb to generate manifests from scala-play-app template

module acumen

appName = "acumen"
appPort = 9200
nodePort = 30200

// LoadBalancer configuration
loadBalancer {
  name = "acumen-lb"
  certificateName = "20210406-2-trueacumen-com-ssl-cert"
  hostname = "api.trueacumen.com"
}
